    "chai": "^4.2.0",
    "danger": "^3.9.0",
    "del": "^3.0.0",
    "eslint": "^5.9.0",
    "eslint-plugin-sw-polymer": "git+https://github.sw-machines.com/IDS/eslint-plugin-sw-polymer.git",
    "git-rev-sync": "^2.0.0",
    "gulp": "^4.0.0",
    "gulp-convert-newline": "0.0.3",
    "gulp-eol": "^0.2.0",
    "gulp-flat": "^0.1.1",
    "gulp-gh-pages": "^0.5.4",
    "gulp-if": "^2.0.2",
    "gulp-inject-string": "^1.1.2",
    "gulp-json-sort": "^1.0.0",
    "gulp-json-transform": "^0.4.5",
    "gulp-merge-json": "^1.3.1",
    "gulp-rename": "^1.4.0",
    "gulp-util": "^3.0.8",
    "gulp-zip": "^4.2.0",
    "jsdoc": "^3.5.5",
    "jsdoc-template": "github:braintree/jsdoc-template",
    "json-multilevel-delta": "0.0.1",
    "json-value-replacer": "^0.1.1",
    "license-report-generator": "^0.2.0",
