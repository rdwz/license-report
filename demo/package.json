    "write-json-file": "^3.0.2",
    "yargs": "^12.0.5"
  },
  "eslintConfig": {
    "extends": [
      "plugin:sw-polymer/polymer-3"
    ],
    "plugins": [
      "sw-polymer"
    ],
    "rules": {
      "max-len": "off"
    }
  },
  "vaadin": {
    "disableUsageStatistics": true
  },
  "main": "index.html",
  "repository": "https://github.sw-machines.com/sroming/ui-platform.git",
  "author": "Sebastian Roming <s.roming@sw-machines.de>, Max Zelger <m.zelgerg@sw-machines.de> ",
  "license": "none"
}
