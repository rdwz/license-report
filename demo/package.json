{
  "name": "ui-platform",
  "version": "3.0.1",
  "contributors": [
    "SW IDS Devs"
  ],
  "scripts": {
    "start": "polymer serve",
    "clean": "npm dedupe",
    "build": "set NODE_OPTIONS=--max_old_space_size=4096 && polymer build",
    "build:gulp": "gulp make",
    "build:gulp:config": "gulp build:config",
    "serve:static": "polymer serve --port 5000 build/es5prod",
    "deploy": "cf push -f build/es5prod/manifest.yml",
    "deploy:documentation": "gulp deploy:documentation",
    "jsdoc": "jsdoc -c jsdoc.json -R README.md -q title=SWlifeData src",
    "lint": "eslint . --ext js --ignore-path .gitignore",
    "lint:src": "eslint ./src --ext html,js",
    "lint:plain": "eslint --ignore-path .gitignore",
    "test": "polymer test --module-resolution=node --npm"
  },
  "dependencies": {
    "@polymer/app-layout": "^3.0.2",
    "@polymer/iron-flex-layout": "^3.0.1",
    "@polymer/iron-form": "^3.0.1",
    "@polymer/iron-icon": "^3.0.1",
    "@polymer/iron-icons": "^3.0.1",
    "@polymer/iron-iconset-svg": "^3.0.1",
    "@polymer/iron-list": "^3.0.2",
    "@polymer/iron-resizable-behavior": "^3.0.1",
    "@polymer/iron-validator-behavior": "^3.0.1",
    "@polymer/marked-element": "^3.0.1",
    "@polymer/neon-animation": "^3.0.1",
    "@polymer/paper-button": "^3.0.1",
    "@polymer/paper-checkbox": "^3.1.0",
    "@polymer/paper-dialog": "^3.0.1",
    "@polymer/paper-dialog-scrollable": "^3.0.1",
    "@polymer/paper-dropdown-menu": "^3.0.1",
    "@polymer/paper-fab": "^3.0.1",
    "@polymer/paper-icon-button": "^3.0.2",
    "@polymer/paper-input": "^3.0.1",
    "@polymer/paper-item": "^3.0.1",
    "@polymer/paper-listbox": "^3.0.1",
    "@polymer/paper-menu-button": "^3.0.1",
    "@polymer/paper-progress": "^3.0.1",
    "@polymer/paper-spinner": "^3.0.2",
    "@polymer/paper-tabs": "^3.1.0",
    "@polymer/paper-toast": "^3.0.1",
    "@polymer/paper-toggle-button": "^3.0.1",
    "@polymer/paper-tooltip": "^3.0.1",
    "@polymer/polymer": "^3.3.0",
    "@vaadin/vaadin-context-menu": "^4.3.12",
    "@vaadin/vaadin-grid": "^5.4.3",
    "browser-detect": "^0.2.28",
    "dayjs": "^1.8.27",
    "dom-flip": "^0.4.3",
    "highcharts": "^7.1.2",
    "i18next": "^11.10.2",
    "leaflet": "^1.4.0",
    "lit-element": "^2.1.0",
    "lit-flexbox-literals": "github:T-Knott-Mesh/lit-flexbox-literals",
    "lit-html": "^1.0.0",
    "moment": "^2.24.0",
    "moment-timezone": "^0.5.25",
    "numeral": "^2.0.6",
    "pwa-helpers": "^0.9.0",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "reselect": "^3.0.1",
    "sweetalert2": "^8.5.0",
    "validator": "^10.11.0"
  },
  "devDependencies": {
    "@polymer/test-fixture": "^4.0.2",
