  "description": "Generates a license report, using your bower/npm dependencies",
  "main": "dist/index.js",
  "scripts": {
    "prepublishOnly": "yarn build",
    "build": "yarn copy:assets && tsc",
    "copy:assets": "copyfiles -f src/assets/**/* dist/assets",
